{
    "version": 8,
    "id": "951209",
    "name": "Bowen",
    "metadata": {
        "maputnik:renderer": "mlgljs"
    },
    "sources": {
        "contours": {
            "type": "vector",
            "tiles": [
                "https://wbjon.github.io/contours/{z}/{x}/{y}.txt"
            ],
            "minzoom": 9,
            "maxzoom": 14
        }
    },
    "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
    "layers": [
        {
            "id": "contour-lines",
            "type": "line",
            "source": "contours",
            "source-layer": "contour",
            "paint": {
                "line-color": "Red",
                "line-opacity": 0.5,
                "line-width": [
                    "match",
                    [
                        "get",
                        "nth_line"
                    ],
                    10,
                    1.5,
                    5,
                    1,
                    0.5
                ]
            }
        },
        {
            "id": "contour-labels",
            "type": "symbol",
            "source": "contours",
            "source-layer": "contour",
            "layout": {
                "text-field": "{height}",
                "text-size": 10,
                "symbol-spacing": 120,
                "symbol-placement": "line"
            },
            "paint": {
                "text-opacity": 0.8,
                "text-color": "#000000"
            }
        }
    ]
}